<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mfc.airquality" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Air Quality</name>
    <description>
    Mini Cordova application for collecting Air Quality measurements.
  </description>
    <author email="info@mappingforchange.org.uk" href="http://mappingforchange.org.uk">
    Mapping for Change
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
